{
  "name": "obsidian-title-forge",
  "version": "1.0.0",
  "description": "TitleForge - Obsidian plugin for automatic note title and tag generation using Gemini API",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "ai",
    "gemini",
    "automation"
  ],
  "author": "shin902",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "@vitest/ui": "^4.0.8",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "jsdom": "^27.2.0",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "vitest": "^4.0.8"
  }
}
